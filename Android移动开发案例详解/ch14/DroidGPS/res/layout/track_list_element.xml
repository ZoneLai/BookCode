<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_height="wrap_content" 
  android:orientation="vertical" 
  android:background="#FFF" 
  android:layout_width="fill_parent">
	<LinearLayout 
	android:id="@+id/LinearLayout01" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content">
		<ImageView 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:id="@+id/iv_tle_states">
		</ImageView>
		<LinearLayout 
		android:id="@+id/LinearLayout02" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:orientation="vertical">
			<TextView 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" 
			android:text="0000-00-00(00:00:00)" 
			android:layout_marginLeft="10px" 
			android:layout_marginBottom="20px" 
			android:textSize="18px" 
			android:id="@+id/tv_tle_date" 
			android:textColor="#000">
			</TextView>
			<LinearLayout 
			android:id="@+id/LinearLayout03" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
				<TextView 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content" 
				android:text="0'00" 
				android:typeface="serif" 
				android:textStyle="normal|italic" 
				android:layout_marginLeft="10px" 
				android:id="@+id/tv_tle_time"
				android:textColor="#000">
				</TextView>
				<TextView 
				android:layout_width="wrap_content" 
				android:layout_height="wrap_content"
				android:textStyle="normal|italic" 
				android:typeface="serif" 
				android:layout_marginLeft="20px" 
				android:text="0.00km" 
				android:id="@+id/tv_tle_distance" 
				android:textColor="#000">
				</TextView>
				<TextView 
				android:layout_width="wrap_content"
				android:layout_height="wrap_content" 
				android:text="0 marks" 
				android:textStyle="normal|italic" 
				android:typeface="serif" 
				android:layout_marginLeft="20px" 
				android:id="@+id/tv_tle_marks" 
				android:textColor="#000">
				</TextView>
			</LinearLayout>
		</LinearLayout>
			<TextView 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content" 
			android:text="Today" 
			android:layout_marginLeft="10px" 
			android:id="@+id/tv_tle_day" 
			android:textColor="#000">
			</TextView>
		</LinearLayout>
	<LinearLayout 
	android:id="@+id/LinearLayout04" 
	android:layout_width="wrap_content"
	android:layout_height="wrap_content">
		<TextView 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:text="No group" 
		android:typeface="serif" 
		android:textStyle="normal|italic" 
		android:layout_marginLeft="60px" 
		android:layout_marginTop="10px" 
		android:id="@+id/tv_group" 
		android:textColor="#000">
		</TextView>
		<TextView 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:textStyle="normal|italic" 
		android:visibility="visible" 
		android:typeface="serif" 
		android:text="0 points" 
		android:layout_marginTop="10px" 
		android:layout_marginLeft="40px" 
		android:id="@+id/tv_tle_points" 
		android:textColor="#000">
		</TextView>
	</LinearLayout>
</LinearLayout>
