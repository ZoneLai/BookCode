<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.xmobileapp.DroidGPS"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/chinaz2" android:label="@string/app_name">
        <activity android:name=".DroidGPS"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GPSStatus">
        </activity>
        <activity android:name=".mapactivity">
        </activity>
        <activity android:name=".TrackList">
        </activity>
        <activity android:name="ViewTrackEdition">
        </activity>
        <activity android:name=".MapViewByAnimation">
        </activity>
        <uses-library android:name="com.google.android.maps"/>
    </application>
    <uses-sdk android:minSdkVersion="3" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" /> 
</manifest> 